{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "lib",
  },
  "include": ["*.ts"]
}
